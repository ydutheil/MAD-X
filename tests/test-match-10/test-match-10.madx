B23:sbend,L=0.9375,Angle=37.5*DEG, K1:=KB23;KB23=1.45520e+000  ;
B24:sbend,L=0.9375,Angle=37.5*DEG, K1:=KB24;KB24=-7.67690e-001 ;
!---------------------------------------------------------------!
D00 : DRIFT, L = 0.00 ;
D01 : DRIFT, L = 0.35 ;
D02 : DRIFT, L = 0.05 ;
D03 : DRIFT, L = 3.80 ;
D04 : DRIFT, L = 1.00 ;
D05 : DRIFT, L = 1.40 ;
D06 : DRIFT, L = 0.75 ;
D12 : DRIFT, L= 0.20  ;
D13 : DRIFT, L= 0.15  ;
D14 : DRIFT, L= 0.4   ;
D15 : DRIFT, L= 0.3   ;
D16 : DRIFT, L= 1.0   ;
D17 : DRIFT, L= 0.25  ;
D18 : DRIFT, L= 0.9   ;
D19 : DRIFT, L= 0.14  ;
D11 : DRIFT, L= 0.05  ;
D10 : DRIFT, L= 0.1   ;
D20 : DRIFT, L= 0.02  ;
D21 : DRIFT, L= 0.01  ;
